@model IEnumerable<MvcApplication1.Models.Room>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            hotelid
        </th>
        <th>
            doornumber
        </th>
        <th>
            price
        </th>
        <th>
            type
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
        <td>
			@item.hotelid
        </td>
        <td>
			@item.doornumber
        </td>
        <td>
			@item.price
        </td>
        <td>
			@item.type
        </td>
    </tr>
}

</table>


